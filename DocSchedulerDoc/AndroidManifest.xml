<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sarangjoshi.docschedulerdoc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="ParseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.facebook.LoginActivity"
            android:label="@string/facebook_login" >
        </activity>
        <activity
            android:name="com.sarangjoshi.docschedulerdoc.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.sarangjoshi.docschedulerdoc.HomeActivity"
            android:label="@string/home_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sarangjoshi.docschedulerdoc.SignupActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.sarangjoshi.docschedulerdoc.LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sarangjoshi.docschedulerdoc.LoginActivity" />
        </activity>
        <activity
            android:name="com.sarangjoshi.docschedulerdoc.EditScheduleActivity"
            android:label="@string/edit_sched_name"
            android:parentActivityName="com.sarangjoshi.docschedulerdoc.HomeActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sarangjoshi.docschedulerdoc.HomeActivity" >
            </meta-data>
        </activity>
        <activity
            android:name="com.sarangjoshi.docschedulerdoc.EditPlaceActivity"
            android:label="@string/edit_place_name"
            android:parentActivityName="com.sarangjoshi.docschedulerdoc.EditScheduleActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sarangjoshi.docschedulerdoc.EditScheduleActivity" >
            </meta-data>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
    </application>

</manifest>